    /* istanbul ignore next */
    cov_16nj2t3cmt().f[5]++;
    cov_16nj2t3cmt().s[53]++;
    logger.error('Express error:', error);
    /* istanbul ignore next */
    cov_16nj2t3cmt().s[54]++;
    res.status(500).json({
      error: 'Internal Server Error'
    });
  });

  // 404 handler
  /* istanbul ignore next */
  cov_16nj2t3cmt().s[55]++;
  app.use((req, res) => {
    /* istanbul ignore next */
    cov_16nj2t3cmt().f[6]++;
    cov_16nj2t3cmt().s[56]++;
