/**
 * Start web server for webhooks and health checks
 */
async function startWebServer(container, config) {
  /* istanbul ignore next */
  cov_16nj2t3cmt().f[4]++;
  const logger =
  /* istanbul ignore next */
  (cov_16nj2t3cmt().s[45]++, container.resolve('logger'));
  const app =
  /* istanbul ignore next */
  (cov_16nj2t3cmt().s[46]++, container.resolve('expressApp'));

  // Set up rate limiting
  const webhookLimiter =
  /* istanbul ignore next */
  (cov_16nj2t3cmt().s[47]++, rateLimit({
    windowMs: 15 * 60 * 1000,
    // 15 minutes
    max: 100,
    // Limit each IP to 100 requests per windowMs
    message: 'Too many requests from this IP',
    standardHeaders: true,
    legacyHeaders: false
  }));
  const commandLimiter =
  /* istanbul ignore next */
  (cov_16nj2t3cmt().s[48]++, rateLimit({
    windowMs: 60 * 1000,
    // 1 minute
    max: 5,
    // Limit each IP to 5 requests per minute
    message: 'Too many commands from this IP',
    standardHeaders: true,
    legacyHeaders: false
  }));

  // Apply rate limiting to webhook endpoints
  /* istanbul ignore next */
  cov_16nj2t3cmt().s[49]++;
  app.use('/youtube-webhook', webhookLimiter);
  /* istanbul ignore next */
  cov_16nj2t3cmt().s[50]++;
  app.use('/api/', commandLimiter);

