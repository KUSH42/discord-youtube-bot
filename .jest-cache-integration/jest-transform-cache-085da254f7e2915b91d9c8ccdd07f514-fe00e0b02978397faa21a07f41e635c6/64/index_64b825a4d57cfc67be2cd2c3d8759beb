async function main() {
  /* istanbul ignore next */
  cov_16nj2t3cmt().f[1]++;
  let container;
  /* istanbul ignore next */
  cov_16nj2t3cmt().s[18]++;
  try {
    /* istanbul ignore next */
    cov_16nj2t3cmt().s[19]++;
    container = await startBot();
    const eventBus =
    /* istanbul ignore next */
    (cov_16nj2t3cmt().s[20]++, container.resolve('eventBus'));
    /* istanbul ignore next */
    cov_16nj2t3cmt().s[21]++;
    eventBus.on('bot.request_restart', async () => {
      /* istanbul ignore next */
      cov_16nj2t3cmt().f[2]++;
      const logger =
      /* istanbul ignore next */
      (cov_16nj2t3cmt().s[22]++, container.resolve('logger'));
