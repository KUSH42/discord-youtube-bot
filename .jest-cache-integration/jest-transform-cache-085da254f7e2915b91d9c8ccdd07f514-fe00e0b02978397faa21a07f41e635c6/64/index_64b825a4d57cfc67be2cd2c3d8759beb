      /* istanbul ignore next */
      cov_16nj2t3cmt().s[24]++;
      await container.dispose();
      /* istanbul ignore next */
      cov_16nj2t3cmt().s[25]++;
      container = await startBot();
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_16nj2t3cmt().s[26]++;
